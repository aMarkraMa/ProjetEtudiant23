<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.projet.controller.ShowBinomeController">
    <children>
        <TableView fx:id="tableviewBinome" layoutX="28.0" layoutY="127.0" prefHeight="253.0" prefWidth="545.0">
        </TableView>
        <Label alignment="CENTER" layoutX="162.0" layoutY="43.0" prefHeight="33.0" prefWidth="303.0"
               text="Gestion des binomes"/>
        <Button fx:id="toAjouterBi" layoutX="485.0" layoutY="82.0" mnemonicParsing="false"
                onAction="#toAjouterBinome" prefHeight="20.0" prefWidth="88.0" text="Ajouter"/>
        <TextField fx:id="textfieldNomEtudiant" layoutX="28.0" layoutY="86.0" prefHeight="20.0" prefWidth="88.0"
                   promptText="Nom"/>
        <TextField fx:id="textfieldPrenomEtudiant" layoutX="117.0" layoutY="86.0" prefHeight="20.0" prefWidth="88.0"
                   promptText="Prenom"/>
        <TextField fx:id="textfieldNomProjet" layoutX="205.0" layoutY="86.0" prefHeight="20.0" prefWidth="88.0"
                   promptText="Projet"/>

        <Button fx:id="searchBinome" layoutX="301.0" layoutY="86.0" mnemonicParsing="false" onAction="#searchBinome"
                prefHeight="20.0" prefWidth="20.0"/>
        <Button fx:id="refreshBinome" layoutX="342.0" layoutY="86.0" mnemonicParsing="false" onAction="#refreshTable"
                prefHeight="20.0" prefWidth="20.0"/>
        <MenuBar layoutX="2.0" layoutY="3.0" prefHeight="38.0" prefWidth="600.0">
            <menus>
                <Menu fx:id="menuBinome" mnemonicParsing="false" text="Menu">
                    <items>
                        <MenuItem fx:id="returnBinome" mnemonicParsing="false" onAction="#retour" text="Retour"/>
                        <MenuItem fx:id="toEtudiants" mnemonicParsing="false" onAction="#toEtudiants"
                                  text="Gestion d'etudiants"/>
                        <MenuItem fx:id="toFormations" mnemonicParsing="false" onAction="#toFormations"
                                  text="Gestion de formations"/>
                        <MenuItem fx:id="toProjets" mnemonicParsing="false" onAction="#toProjets"
                                  text="Gestion de projets"/>
                        <MenuItem fx:id="toNotes" mnemonicParsing="false" onAction="#toNotes" text="Gestion de notes"/>
                    </items>
                </Menu>
                <Menu fx:id="downloadBinome" mnemonicParsing="false" text="Download"/>
            </menus>
        </MenuBar>
    </children>
</AnchorPane>
