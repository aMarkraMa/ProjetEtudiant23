<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="522.0" prefWidth="784.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.projet.controller.ShowProjetController">
    <children>
        <TableView fx:id="tableViewProjet" layoutX="10.0" layoutY="121.0" prefHeight="253.0" prefWidth="545.0" AnchorPane.bottomAnchor="26.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="121.0">
            <columns>
                <TableColumn fx:id="nomMatiere" prefWidth="134.0" text="nomMatière" />
                <TableColumn fx:id="sujet" prefWidth="130.0" text="sujet" />
                <TableColumn fx:id="datePrevueRemise" minWidth="0.0" prefWidth="131.0" text="datePrévueRemise" />
                <TableColumn fx:id="pourcentageSoutenance" minWidth="0.0" prefWidth="149.0" text="pourcentageSoutenance" />
            </columns>
        </TableView>
        <Label alignment="CENTER" layoutX="161.0" layoutY="38.0" prefHeight="33.0" prefWidth="303.0" text="Gestion des projets" AnchorPane.leftAnchor="161.0" AnchorPane.rightAnchor="136.0" AnchorPane.topAnchor="38.0" />
        <Button fx:id="addProjet" layoutX="412.0" layoutY="75.0" mnemonicParsing="false" onAction="#showAddView" prefHeight="27.0" prefWidth="45.0" AnchorPane.rightAnchor="143.0" AnchorPane.topAnchor="75.0" />
        <TextField fx:id="textFieldNomMatiere" layoutX="28.0" layoutY="77.0" prefHeight="27.0" prefWidth="88.0" promptText="nomMatiere" AnchorPane.leftAnchor="28.0" AnchorPane.topAnchor="75.0" />
        <Button fx:id="searchProjet" layoutX="356.0" layoutY="75.0" mnemonicParsing="false" onAction="#searchProjet" prefHeight="27.0" prefWidth="45.0" AnchorPane.rightAnchor="199.0" AnchorPane.topAnchor="75.0" />
        <Button fx:id="refreshProjet" layoutX="528.0" layoutY="75.0" mnemonicParsing="false" onAction="#refreshTable" prefHeight="27.0" prefWidth="45.0" AnchorPane.rightAnchor="27.0" AnchorPane.topAnchor="75.0" />
        <Button fx:id="deleteProjet" layoutX="470.0" layoutY="75.0" mnemonicParsing="false" onAction="#deleteProjet" prefHeight="27.0" prefWidth="45.0" AnchorPane.rightAnchor="85.0" AnchorPane.topAnchor="75.0" />
        <TextField fx:id="textFieldSujet" layoutX="133.0" layoutY="75.0" prefHeight="27.0" prefWidth="83.0" promptText="sujet" AnchorPane.leftAnchor="133.0" AnchorPane.topAnchor="75.0" />
        <DatePicker fx:id="datePicker" layoutX="236.0" layoutY="74.0" prefHeight="27.0" prefWidth="98.0" AnchorPane.leftAnchor="236.0" AnchorPane.topAnchor="75.0" editable="false"/>

        <MenuBar layoutX="2.0" layoutY="3.0" prefHeight="38.0" prefWidth="600.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <menus>
                <Menu fx:id="menuProjet" mnemonicParsing="false" text="Menu">
                    <items>
                        <MenuItem fx:id="returnFormation" mnemonicParsing="false" onAction="#retour" text="Retour" />
                        <MenuItem fx:id="toShowEtudiant" mnemonicParsing="false" onAction="#toShowEtudiant" text="Etudiant" />
                        <MenuItem fx:id="toShowFormation" mnemonicParsing="false" onAction="#toShowFormation" text="Formation" />
                        <MenuItem fx:id="toProjets" mnemonicParsing="false" onAction="#toShowProjet" text="Projet" />
                        <MenuItem fx:id="toBinomes" mnemonicParsing="false" onAction="#toShowBinome" text="Binome" />
                        <MenuItem fx:id="toNotes" mnemonicParsing="false" onAction="#toShowNote" text="Note" />
                    </items>
                </Menu>
                <Menu fx:id="file" mnemonicParsing="false" text="File" >
                    <items>
                        <MenuItem fx:id="downloadPDF" mnemonicParsing="false" onAction="#toPDF" text="Telecharger le PDF"/>
                        <MenuItem fx:id="downloadExcel" mnemonicParsing="false" onAction="#toExcel" text="Telecharger l'excel"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>

    </children>
</AnchorPane>
