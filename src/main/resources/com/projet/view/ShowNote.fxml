                <TableColumn fx:id="nomEtudiant" minWidth="20.0" prefWidth="87.0" text="nom" />
                <TableColumn fx:id="prenomEtudiant" minWidth="20.0" prefWidth="106.0" text="prenome" />
                <TableColumn fx:id="noteSoutenance" minWidth="20.0" prefWidth="59.0" text="noteSout" />
                <TableColumn fx:id="noteRapport" minWidth="20.0" prefWidth="65.0" text="noteRapp" />
                <TableColumn fx:id="noteFinale" minWidth="20.0" prefWidth="54.0" text="noteFin" />
            </columns>
        </TableView>
        <Label alignment="CENTER" layoutX="241.0" layoutY="46.0" prefHeight="33.0" prefWidth="303.0" text="Gestion des notes" />
        <Button fx:id="searchNote" layoutX="632.0" layoutY="86.0" mnemonicParsing="false" onAction="#searchNote" prefHeight="27.0" prefWidth="45.0" AnchorPane.rightAnchor="107.0" AnchorPane.topAnchor="86.0" />
        <Button fx:id="refreshNote" layoutX="707.0" layoutY="86.0" mnemonicParsing="false" onAction="#refreshTable" prefHeight="27.0" prefWidth="45.0" AnchorPane.rightAnchor="32.0" AnchorPane.topAnchor="86.0" />
        <TextField fx:id="textFieldNomEtudiant" layoutX="291.0" layoutY="86.0" prefHeight="27.0" prefWidth="85.0" promptText="Nom" />
        <TextField fx:id="textFieldPrenomEtudiant" layoutX="409.0" layoutY="86.0" prefHeight="27.0" prefWidth="103.0" promptText="Prenom" />
        <TextField fx:id="textFieldNomMatiere" layoutX="28.0" layoutY="86.0" prefHeight="27.0" prefWidth="93.0" promptText="Matiere" />
        <TextField fx:id="textFieldSujet" layoutX="144.0" layoutY="86.0" prefHeight="27.0" prefWidth="126.0" promptText="Sujet" />

        <MenuBar layoutX="2.0" layoutY="3.0" prefHeight="38.0" prefWidth="600.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
