<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="522.0" prefWidth="784.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.projet.controller.ShowNoteController">
    <children>
        <TableView fx:id="tableViewNote" layoutX="28.0" layoutY="127.0" prefHeight="367.0" prefWidth="724.0">
