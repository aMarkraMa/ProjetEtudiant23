<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.projet.controller.ShowFormationController">
    <children>
        <TableView fx:id="tableviewFormation" layoutX="28.0" layoutY="127.0" prefHeight="253.0" prefWidth="545.0">
        </TableView>
        <Label alignment="CENTER" layoutX="162.0" layoutY="43.0" prefHeight="33.0" prefWidth="303.0" text="Gestion de formations" />
        <Button fx:id="toAjouterFor" layoutX="485.0" layoutY="82.0" mnemonicParsing="false" onAction="#toAjouterFormation" prefHeight="20.0" prefWidth="88.0" text="Ajouter" />
        <TextField fx:id="textfieldNomFormation" layoutX="28.0" layoutY="86.0" prefHeight="20.0" prefWidth="88.0" promptText="Formation" />
        <TextField fx:id="textfieldPromotion" layoutX="117.0" layoutY="86.0" prefHeight="20.0" prefWidth="88.0" promptText="Promotion" />
        <Button fx:id="searchFormation" layoutX="208.0" layoutY="86.0" mnemonicParsing="false" onAction="#searchFormation" prefHeight="20.0" prefWidth="20.0" />
        <Button fx:id="refreshFormation" layoutX="249.0" layoutY="86.0" mnemonicParsing="false" onAction="#refreshTable" prefHeight="20.0" prefWidth="20.0" />
        <MenuBar layoutX="2.0" layoutY="3.0" prefHeight="38.0" prefWidth="600.0">
            <menus>
                <Menu fx:id="menuFormation" mnemonicParsing="false" text="Menu">
                    <items>
                        <MenuItem fx:id="toEtudiants" mnemonicParsing="false" onAction="#toEtudiants" text="Gestion d'etudiants" />
                        <MenuItem fx:id="toProjets" mnemonicParsing="false" onAction="#toProjets" text="Gestion de projets" />
                        <MenuItem fx:id="toBinomes" mnemonicParsing="false" onAction="#toBinomes" text="Gestion de binomes" />
                        <MenuItem fx:id="toNotes" mnemonicParsing="false" onAction="#toNotes" text="Gestion de notes" />
                    </items>
                </Menu>
                <Menu fx:id="downloadFormation" mnemonicParsing="false" text="Download" />
            </menus>
        </MenuBar>
    </children>
</AnchorPane>
